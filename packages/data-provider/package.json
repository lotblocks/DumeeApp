{
  "name": "dumee-data-provider",
  "version": "0.7.902",
  "description": "data services for dumee apps",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./react-query": {
      "import": "./dist/react-query/index.es.js",
      "require": "./dist/react-query/index.js",
      "types": "./dist/types/react-query/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && vite build",
    "build:watch": "vite build --watch",
    "test": "jest --coverage --watch",
    "test:ci": "jest --coverage --ci",
    "verify": "npm run test:ci",
    "b:clean": "bun run rimraf dist",
    "b:build": "bun run b:clean && bun run rollup -c --silent --bundleConfigAsCjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/dumee.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-username/dumee/issues"
  },
  "homepage": "https://dum.ee",
  "dependencies": {
    "axios": "^1.8.2",
    "dayjs": "^1.11.13",
    "js-yaml": "^4.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@langchain/core": "^0.3.62",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-replace": "^5.0.5",
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.3.0",
    "@types/react": "^18.2.18",
    "@types/winston": "^2.4.4",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "openapi-types": "^12.1.3",
    "rimraf": "^5.0.1",
    "typescript": "^5.0.4",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^4.28.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
