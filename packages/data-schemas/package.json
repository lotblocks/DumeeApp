{
  "name": "@dumee/data-schemas",
  "version": "0.0.12",
  "description": "Mongoose schemas and models for Dumee",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && vite build",
    "build:watch": "vite build --watch",
    "test": "jest --coverage --watch",
    "test:ci": "jest --coverage --ci",
    "verify": "npm run test:ci",
    "b:clean": "bun run rimraf dist",
    "b:build": "bun run b:clean && bun vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/dumee.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-username/dumee/issues"
  },
  "homepage": "https://dum.ee",
  "devDependencies": {
    "@types/diff": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.3.0",
    "@types/traverse": "^0.6.37",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "mongodb-memory-server": "^10.1.4",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.4",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "jsonwebtoken": "^9.0.2",
    "keyv": "^5.3.2",
    "klona": "^2.0.6",
    "dumee-data-provider": "*",
    "lodash": "^4.17.21",
    "meilisearch": "^0.38.0",
    "mongoose": "^8.12.1",
    "nanoid": "^3.3.7",
    "traverse": "^0.6.11",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "keywords": [
    "mongoose",
    "schema",
    "typescript",
    "dumee"
  ]
}